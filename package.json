{
  "name": "atom-htmltidy",
  "main": "./lib/init",
  "version": "4.0.0",
  "description": "Lint and Format HTML using Tidy HTML 5",
  "keywords": [
    "html",
    "format",
    "beautify",
    "linter",
    "prettify",
    "style",
    "tidy"
  ],
  "repository": "https://github.com/AnWeber/atom-htmltidy",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  },
  "dependencies": {
    "htmltidy": "0.0.6",
    "lodash": "^3.10.1"
  },
  "devDependencies": {
    "jsdoc": "^3.4.0"
  },
  "scripts": {
    "start": "jsdoc lib -r -d doc"
  }
}
