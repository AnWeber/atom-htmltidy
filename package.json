{
  "name": "atom-htmltidy",
  "main": "./lib/init",
  "version": "1.6.0",
  "description": "Format HTML using Tidy HTML 5",
  "keywords": [
    "html",
    "format",
    "beautify",
    "prettify",
    "style",
    "tidy"
  ],
  "repository": "https://github.com/AnWeber/atom-htmltidy",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "activationCommands": {
    "atom-workspace": [
      "core:save",
      "atom-htmltidy:format"
    ]
  },
  "dependencies": {
    "htmltidy": "0.0.6",
    "lodash": "^3.10.1"
  }
}
